    "def tree(n):\n",
    "    space_0 = int((n/2)+1)\n",
    "    for i in range(1, n+1):\n",
    "        for j in range(space_0-i+2):\n",
    "            print(\" \", end='')\n",
    "        for k in range(1, i+1):\n",
    "            print(\"* \", end='')\n",
    "        \n",
    "        print(\"\\n\")"
   ]
  },
